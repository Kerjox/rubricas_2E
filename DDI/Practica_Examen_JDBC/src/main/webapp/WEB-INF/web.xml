<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">


	<servlet>
		<servlet-name>titulacionServlet</servlet-name>
		<servlet-class>com.kerjox.practica_examen_jdbc.servlets.InsertTitulacionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>titulacionServlet</servlet-name>
		<url-pattern>insert-titulacion</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>formInsertAlumnoServletDao</servlet-name>
		<servlet-class>com.kerjox.practica_examen_jdbc.servlets.FormInsertAlumnoServletDao</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>formInsertAlumnoServletDao</servlet-name>
		<url-pattern>form-insert-alumno</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>insertAlumno</servlet-name>
		<servlet-class>com.kerjox.practica_examen_jdbc.servlets.InsertAlumno</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>insertAlumno</servlet-name>
		<url-pattern>insert-alumno</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>listAlumnosServlet</servlet-name>
		<servlet-class>com.kerjox.practica_examen_jdbc.servlets.ListAlumnosServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>listAlumnosServlet</servlet-name>
		<url-pattern>list-alumnos</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>editAlumnoServletDao</servlet-name>
		<servlet-class>com.kerjox.practica_examen_jdbc.servlets.EditAlumnoServletDao</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>editAlumnoServletDao</servlet-name>
		<url-pattern>edit-alumno</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>editAlumnoServlet</servlet-name>
		<servlet-class>com.kerjox.practica_examen_jdbc.servlets.EditAlumnoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>editAlumnoServlet</servlet-name>
		<url-pattern>update-alumno</url-pattern>
	</servlet-mapping>

</web-app>